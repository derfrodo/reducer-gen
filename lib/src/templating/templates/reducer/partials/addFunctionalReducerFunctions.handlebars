
    // cases for properties using functional parameters (will return original state, if result does not change)
{{#each state.properties}}
        case actions.{{baseActionEnumValue}}_FUNCTIONAL:
        {
            {{!-- {{json this}} --}}
            const next = action.next(state);
            return next !== state.{{name}} ? {
                ...state,
                {{name}}: next,
            } : state;
        }
{{/each}}