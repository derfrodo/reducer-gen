{
  "image": "mcr.microsoft.com/devcontainers/typescript-node",
  "customizations": {
    "vscode": {
      "extensions": []
    }
  },
  "workspaceFolder": "/workspace",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached",
  "mounts": [
    "type=volume,source=reducer-gen-examples-webapp-cra-node_modules,target=/workspace/examples/webapp-cra/node_modules",
    "type=volume,source=reducer-gen-examples-demo-node_modules,target=/workspace/examples/demo/node_modules",
    "type=volume,source=reducer-gen-lib-node_modules,target=/workspace/lib/node_modules"
  ],
  "postCreateCommand": "sudo chown node:node /workspace/examples/webapp-cra/node_modules && sudo chown node:node /workspace/examples/demo/node_modules && sudo chown node:node /workspace/lib/node_modules",
  "forwardPorts": [
    3000
  ]
}